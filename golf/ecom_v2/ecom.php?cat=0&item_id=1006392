           function updatePrice() {
              var price = getPriceForDiscountLevel($('#quantity').val());
              if(price)
                 $('#price').html("<div class=ecom2price_sale_price><b>$ " + price + "</b></div>");
              else
                 $('#price').html("<div style='padding:25px 0 50px 0;'><label class='control-label col-md-3 col-sm-3 col-xs-12 col-lg-3'><h2>Price</h2></label><div class ='col-md-9 col-sm-9 col-xs-12 col-lg-9' style='padding: 0 0 0 0'>$200.00</div></div>");
           }
           function updateAddOnPrice(spanID, quantity, modifier) {
              addOnTotal = quantity * modifier;
              addOnTotal = parseFloat(addOnTotal).toFixed(2);
              $('#'+spanID).html(addOnTotal);
           }
